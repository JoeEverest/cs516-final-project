    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.0.0",
    "eslint": "^9.29.0",
    "globals": "^16.2.0",
    "serverless": "^3.38.0",
    "serverless-offline": "^13.3.0",
